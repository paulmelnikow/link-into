{
  "name": "link-into",
  "description": "Create a tree of symlinks from glob patterns",
  "keywords": ["symlink"],
  "version": "1.0.1",
  "license": "MIT",
  "main": "link-into.js",
  "bin": {
    "link-into": "cli.js"
  },
  "scripts": {
    "jscs": "jscs --config node_modules/bodylabs-javascript-style/jscs/jscsrc-shared *.js",
    "eslint": "eslint --config node_modules/bodylabs-javascript-style/eslint/eslintrc-shared.yml '*.js'",
    "lint": "npm run jscs && npm run eslint",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:paulmelnikow/link-into.git"
  },
  "dependencies": {
    "es6-promisify": "^4.0.0",
    "glob": "^7.0.3",
    "mkdirp": "^0.5.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "bodylabs-javascript-style": "^5.0.0",
    "eslint": "2.9.0",
    "jscs": "^2.11.0"
  }
}
