{
  "name": "link-into",
  "description": "Create a tree of symlinks from glob patterns",
  "keywords": [
    "symlink",
    "glob",
    "cli"
  ],
  "version": "3.0.1",
  "license": "MIT",
  "main": "link-into.js",
  "bin": {
    "link-into": "cli.js"
  },
  "files": [
    "*.js",
    "!test.js"
  ],
  "scripts": {
    "lint": "eslint \"**/*.js\"",
    "prettier": "prettier --write \"**/*.@(js|md|yml)\"",
    "prettier:check": "prettier --check \"**/*.@(js|md|yml)\"",
    "mocha": "mocha test.js",
    "test": "npm run mocha && npm run lint && npm run prettier:check"
  },
  "repository": "paulmelnikow/link-into",
  "engines": {
    "node": ">= 10.0"
  },
  "dependencies": {
    "glob": "^7.0.3",
    "mkdirp": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.3.0",
    "prettier": "2.2.1",
    "tmp-promise": "^3.0.2"
  }
}
